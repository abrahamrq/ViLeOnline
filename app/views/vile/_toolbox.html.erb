<xml id="toolbox" style="display: none">
  <category name="Functions">
    <block type='vile_main'></block>
    <block type='vile_function'></block>
    <block type='vile_parameter'></block>
    <block type='vile_return'></block>
    <block type='vile_function_call'></block>
    <block type='vile_function_call_statute'></block>
    <block type='vile_input_parameter'></block>
  </category>
  <category name="Conditions">
    <block type='vile_if'></block>
    <block type='vile_if_else'></block>
    <block type='vile_bool'></block>
